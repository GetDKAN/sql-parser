{
    "name": "getdkan/sql-parser",
    "description": "A state-machine-driven parser and validator for SQL-like strings.",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "fmizzell",
            "email": "fmizzell.dev@gmail.com"
        }
    ],
    "require": {
        "fmizzell/maquina": "^1.0.0"
    },
    "require-dev": {
        "bugatino/phpcs-git-pre-commit": "dev-master",
        "phpunit/phpunit": ">=8.3 <8.5 || >8.5.14 <9.5",
        "rector/rector": "^0.15.17",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "SqlParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SqlParserTest\\": "test/"
        }
    },
    "scripts": {
        "phpcbf": "./vendor/bin/phpcbf",
        "phpcs": "./vendor/bin/phpcs",
        "post-install-cmd": [
            "sh ./vendor/bugatino/phpcs-git-pre-commit/src/setup.sh"
        ],
        "post-update-cmd": [
            "sh ./vendor/bugatino/phpcs-git-pre-commit/src/setup.sh"
        ],
        "rector": "./vendor/bin/rector process",
        "rector-dry-run": "./vendor/bin/rector process --dry-run",
        "test": "./vendor/bin/phpunit --testsuite all"
    }
}
